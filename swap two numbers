#include <stdio.h>

int main() {
    int num1, num2, temp;

    printf("Enter value of num1: ");
    scanf("%d", &num1);

    printf("Enter value of num2: ");
    scanf("%d", &num2);

    // Swapping logic
    temp = num1;
    num1 = num2;
    num2 = temp;

    printf("\nAfter swapping, the value of num1 is %d\n", num1);
    printf("After swapping, the value of num2 is %d\n", num2);

    return 0;
}
